{
    "env": {
        "es2021": true,
        "node": true
    },
    "globals": {
        "document": true,
        "fetch": true,
        "window": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ]
    }
}
